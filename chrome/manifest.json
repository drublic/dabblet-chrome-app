{
  "name": "dabblet",
  "description": "An interactive CSS playground with extensive GitHub gist-use.",
  "version": "0.70",
  "icons": {
    "16": "app/icons/dabblet-16x16.png",
    "48": "app/icons/dabblet-48x48.png",
    "128": "app/icons/dabblet-128x128.png"
  },
  "content_scripts": [{
    "matches": ["https://gist.github.com/*"],
    "js": ["app/js/github/manipulation.js"]
  }],
  "app": {
    "launch": {
       "local_path" : "app/main.html"
    }
  }
}